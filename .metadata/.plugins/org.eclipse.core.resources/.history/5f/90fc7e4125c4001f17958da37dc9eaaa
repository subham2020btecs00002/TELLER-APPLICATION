package com.example.bank.dto;

import lombok.AllArgsConstructor;
import lombok.Data;

@Data
@AllArgsConstructor
public class AccountInfoDTO {
    private Long accountId;
    private Long customerId;
    private String customerName;  // Add customer name
    private String customerEmail; // Add customer email
    private String customerPhoneNumber; // Add customer phone number
    private String customerGender; // Add customer gender
    private String customerAddress; // Add customer address
    private double openingBalance;
    private double closingBalance;
    private String accountNumber;
}
